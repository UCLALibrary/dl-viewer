{
  "name": "dl-viewer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'CYPRESS_BASE_URL=http://localhost:4173 cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'CYPRESS_BASE_URL=http://localhost:4173 cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "copy-uv": "rimraf public/uv && cp -R node_modules/universalviewer/dist public/uv",
    "postinstall": "npm run copy-uv"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.0",
    "axios": "^1.3.4",
    "cypress": "^12.7.0",
    "cypress-iframe": "^1.0.1",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jquery": "^3.6.4",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "universalviewer": "^3.1.4",
    "video.js": "^8.0.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-gtm": "^3.5.0"
  }
}
