{
  "name": "dl-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "copy-uv": "rimraf public/uv && cp -R node_modules/universalviewer/dist public/uv",
    "copy-mejs": "cp -R node_modules/mediaelement/build public/mejs",
    "copy-jquery": "cp -R node_modules/jquery/dist public/jquery",
    "postinstall": "npm run copy-uv && npm run copy-mejs && npm run copy-jquery"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "hls.js": "^0.12.4",
    "jquery": "^3.3.1",
    "mediaelement": "^4.2.16",
    "universalviewer": "^3.1.4",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.2.1"
  }
}
